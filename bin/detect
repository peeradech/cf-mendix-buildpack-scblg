#!/usr/bin/env bash

cd "$1"

if [[ -d model && -d web && -f model/metadata.json ]]; then
    echo "Mendix Cloud Foundry Buildpack"
    exit 0
elif [[ -d userlib && -d javasource && -d theme && -n "*.mpr" ]]; then
    echo "Mendix Cloud Foundry Buildpack"
    exit 0
else
    echo "no"
    exit 1
fi
